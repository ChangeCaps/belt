let max = 0;

while true {
	let index = 0;
	let crates = crates();

	let len = len(crates);

	let min = 0;
	let found = false;

	while index < len {
		if crates[index].type == "crate_1" {
			if crates[index].id > max {
				if !found {
					found = true;
					min = index;
				}

				if crates[index].id < crates[min].id {
					min = index;
				}
			}
		}

		index = index + 1;
	}

	if found {
		max = crates[min].id;
		grab(crates[min].id);
		drop(1, 1);
	}
}
